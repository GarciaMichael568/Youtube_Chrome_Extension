{
    "manifest_version": 2,
    "name": "YoutubeFriends",
    "description": "An extension that allows you to see your friends favorite videos!",
    "version": "1.0",
    "icons": {
        "16": "/images/icon16.png",
        "48": "/images/icon48.png",
        "128": "/images/icon128.png"
    },
    "page_action": {
        "default_icon": {
            "16": "/images/icon16.png",
            "48": "/images/icon48.png",
            "128": "/images/icon128.png"
        }
    }
}