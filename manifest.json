{
    "manifest_version": 3,
    "name": "Youtube History",
    "description": "An extension that displays youtube history",
    "version": "1.0.0",
    "icons": {
        "128": "/images/icon128.png"
    },
   "browser_action":{
       "default_icon": "/images/icon.png",
       "default_popup": "popup.html"
   },
   "permissions": ["activeTab"]
}